{
  "name": "Vald",
  "dockerFile": "./Dockerfile",
  "context": "..",
  "runArgs": ["--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined"],
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2": {
			"version": "latest",
			"enableNonRootDocker": "true",
			"moby": "true"
		}
	},
  "postCreateCommand": "go version"
}
